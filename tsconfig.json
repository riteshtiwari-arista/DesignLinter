{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "types": ["@figma/plugin-typings"],
    "outDir": "dist"
  },
  "include": ["src"],
  "exclude": ["src/ui"]
}
